<!DOCTYPE html>
<html lang="en">
<head>
  <title>User Profile</title>
</head>
<body>
  <%- include('./partials/nav.ejs') %>
  <div class="content">
    <h1><%= user.name %>'s Profile</h1>
    <p>Email: <%= user.email %></p>
    <h2>Hobbies</h2>
    <ul>
      <% user.hobbies.forEach(function(hobby){ %>
        <li><%= hobby %></li>
      <% }); %>
    </ul>
  </div>
  <%- include('./partials/logout.ejs') %>
</body>
</html>